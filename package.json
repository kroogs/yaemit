  "keywords": [
    "events",
    "event",
    "emit",
    "emitter",
    "eventemitter"
  ],
  "homepage": "https://github.com/kroogs/yaemit",
  "author": "Justin Krueger <justin@kroo.gs>",
  "contributors": [],
  "private": "true",
  "license": "MIT",
  "main": "dist/yaemit.js",
  "files": [
    "dist",
    "doc",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kroogs/yaemit.git"
  },
  "bugs": {
    "url": "https://github.com/kroogs/yaemit/issues"
  },
  "scripts": {
    "start": "webpack",
    "build": "webpack -p",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
